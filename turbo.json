{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "format-and-lint": {},
    "format-and-lint:fix": {
      "cache": false
    },
    "server#build": {
      "dependsOn": ["@fuseui/core#build", "@fuseui/sdk#build"]
    },
    "web#build": {
      "dependsOn": ["@fuseui/sdk#build"]
    },
    "@fuseui/cli#build": {
      "dependsOn": ["@fuseui/core#build", "@fuseui/sdk#build"]
    }
  }
}
