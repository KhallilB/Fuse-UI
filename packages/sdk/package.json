{
  "name": "@fuseui/sdk",
  "version": "0.2.1",
  "private": false,
  "description": "FuseUI SDK for API integration",
  "author": "FuseUI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KhallilB/FuseUI.git",
    "directory": "packages/sdk"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "turbo run format-and-lint",
    "lint:fix": "turbo run format-and-lint:fix",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "@fuseui/typescript-config": "workspace:*",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
}
