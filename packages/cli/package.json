{
  "name": "@fuseui-org/cli",
  "version": "0.1.1",
  "private": false,
  "type": "module",
  "description": "FuseUI Command Line Interface",
  "author": "FuseUI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KhallilB/FuseUI.git",
    "directory": "packages/cli"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "fuseui": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "build": "tsup src/index.ts --format esm --dts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "turbo run format-and-lint",
    "lint:fix": "turbo run format-and-lint:fix",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@fuseui/core": "workspace:*",
    "@fuseui-org/sdk": "workspace:*",
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@fuseui/typescript-config": "workspace:*",
    "@types/node": "^20.11.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
}
